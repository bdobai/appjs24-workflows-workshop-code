{
  "cli": {
    "version": ">= 9.0.8"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true
      },
      "env": { 
        "APP_VARIANT": "development" 
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production:pretend": {
      "channel": "production",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
